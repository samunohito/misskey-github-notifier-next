{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "misskey-github-notifier",
  "main": "src/index.ts",
  "compatibility_date": "2023-12-01",
  "dev": {
    "port": 8080
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "CONFIG": {
      "notifyTo": {
        "misskey": {
          "enabled": true,
          "defaultPostVisibility": "public"
        }
      }
    }
  }
}
